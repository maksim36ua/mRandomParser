@model IEnumerable<string>

<form asp-controller="Home" asp-action="Link" method="post" class="form-horizontal" role="form">
    <div class="form-horizontal">
        <div asp-validation-summary="ValidationSummary.All" class="text-danger"></div>
        <div class="form-group">
            <input type="text" name="website" value="" placeholder="Enter mRandom link"/>            
        </div>
        <div class="form-group">
            <input type="submit" value="Pull winners" class="btn btn-default" />
        </div>
    </div>
</form>

<table class="table">
    <tr>
        <th>Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item)
            </td>
            
        </tr>
    }
</table>
